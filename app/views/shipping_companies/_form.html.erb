<%= form_with(model: @shipping_company) do |f| %>
  <div>
    <%= f.label :corporate_name %>
    <%= f.text_field :corporate_name %>
    <%= @shipping_company.errors.full_messages_for(:corporate_name)[0] if @shipping_company.errors.full_messages_for(:corporate_name).any? %>
  </div>
  <div>
    <%= f.label :brand_name %>
    <%= f.text_field :brand_name %>
    <%= @shipping_company.errors.full_messages_for(:brand_name)[0] if @shipping_company.errors.full_messages_for(:brand_name).any? %>
  </div>
  <div>
    <%= f.label :registration_number %>
    <%= f.text_field :registration_number %>
    <%= @shipping_company.errors.full_messages_for(:registration_number)[0] if @shipping_company.errors.full_messages_for(:registration_number).any? %>
  </div>
  <div>
    <%= f.label :email_domain %>
    <%= f.text_field :email_domain %>
    <%= @shipping_company.errors.full_messages_for(:email_domain)[0] if @shipping_company.errors.full_messages_for(:email_domain).any? %>
  </div>
    <div>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= @shipping_company.errors.full_messages_for(:address)[0] if @shipping_company.errors.full_messages_for(:address).any? %>
  </div>
  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>